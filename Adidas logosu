import turtle

t = turtle.Turtle()

import time
t.speed(4)
t.up()
t.goto(-135,0)

t.begin_fill()
t.left(120)
t.down()
t.fd(30)
t.rt(90)
t.fd(50)
t.rt(90)
t.fd(60)
t.rt(120)
t.fd(58)
t.end_fill()

t.up()
t.rt(180)
t.fd(80)

t.begin_fill()
t.left(120)
t.down()
t.fd(80)
t.rt(90)
t.fd(80)
t.rt(90)
t.fd(126)
t.rt(120)
t.fd(94)
t.end_fill()

t.up()
t.rt(180)
t.fd(115)

t.begin_fill()
t.left(120)
t.down()
t.fd(145)
t.rt(90)
t.fd(90)
t.rt(90)
t.fd(195)
t.rt(120)
t.fd(110)
t.end_fill()
time.sleep(5)
